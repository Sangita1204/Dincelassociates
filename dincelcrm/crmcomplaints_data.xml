<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!-- subtypes for messaging / Chatter -->
        <record id="mt_complaint_pending" model="mail.message.subtype">
            <field name="name">Complaint to Review</field>
            <field name="res_model">dincelcrm.complaints</field>
            <field name="default" eval="False"/>
            <field name="description">Complaint pending</field>
        </record>
        <record id="mt_complaint_closed" model="mail.message.subtype">
            <field name="name">Complaint Closed</field>
            <field name="res_model">dincelcrm.complaints</field>
            <field name="default" eval="False"/>
            <field name="description">Complaint closed</field>
        </record>
        <record id="mt_complaint_opened" model="mail.message.subtype">
            <field name="name">Complaint Opened</field>
            <field name="res_model">dincelcrm.complaints</field>
            <field name="default" eval="False"/>
            <field name="description">Complaint opened</field>
        </record>

    </data>
</openerp>
